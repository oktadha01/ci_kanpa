    <script>
    // Swiper Configuration
    var swiper = new Swiper(".swiper-container-bs", {
        slidesPerView: 1.5,
        spaceBetween: 10,
        centeredSlides: true,
        freeMode: true,
        grabCursor: true,
        loop: true,
        pagination: {
            el: ".swiper-pagination-bullet-bs",
            clickable: true
        },
        autoplay: {
            delay: 4000,
            disableOnInteraction: false
        },
        navigation: {
            nextEl: ".swiper-button-next-bs",
            prevEl: ".swiper-button-prev-bs"
        },
        breakpoints: {
            500: {
                slidesPerView: 2
            },
            700: {
                slidesPerView: 2.3
            }
        }
